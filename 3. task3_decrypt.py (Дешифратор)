import sys

def decrypt(s):
    res = []
    i = 0
    while i < len(s):
        if s[i:i+2] == "..":
            if res:
                res.pop()
            i += 2
        elif s[i] == ".":
            i += 1
        else:
            res.append(s[i])
            i += 1
    return "".join(res)

if __name__ == "__main__":
    text = sys.stdin.read().strip()
    print(decrypt(text))
